{
  "name": "blockchain",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prettier": "prettier \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format": "yarn prettier --write",
    "start": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,__tests__}/**/*.ts\"",
    "test": "jest --config ./__tests__/jest.config.js",
    "test:cov": "yarn test --coverage",
    "test:e2e": "jest --config ./__tests__/jest-e2e.config.js",
    "prepare": "husky install",
    "get:secrets": "aws secretsmanager get-secret-value --region us-east-1 --secret-id .env | jq --raw-output '.SecretString' > .env",
    "put:secrets": "aws secretsmanager put-secret-value --region us-east-1 --secret-id .env --secret-string file://.env"
  },
  "lint-staged": {
    "**/*": "yarn lint",
    "package.json": "sort-package-json"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@solana/web3.js": "^1.73.0",
    "@types/passport-steam": "^1.0.1",
    "axios": "^0.27.2",
    "bs58": "^5.0.0",
    "caip": "^1.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-session": "^1.17.2",
    "helmet": "^6.0.1",
    "mongodb": "^4.13.0",
    "mongoose": "^6.8.1",
    "mongoose-autopopulate": "^0.17.0",
    "mongoose-delete": "^0.5.4",
    "nest-router": "^1.0.9",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-discord": "^0.1.4",
    "passport-steam": "^1.0.17",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^8.0.0",
    "@types/connect-redis": "^0.0.19",
    "@types/cookie-parser": "^1.4.2",
    "@types/eslint": "8.4.10",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.0.2",
    "@types/mongoose-delete": "^0.5.9",
    "@types/node": "18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-discord": "^0.1.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "~5.47.1",
    "@typescript-eslint/parser": "~5.47.1",
    "ajv": "^8.11.2",
    "eslint": "~8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^28.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "sort-package-json": "^2.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "typescript": "4.9"
  }
}
